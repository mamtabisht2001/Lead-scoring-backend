// schema.prisma
generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Offer {
    id            String   @id @default(uuid())
    name          String
    valueProps    String[]
    idealUseCases String[]
    createdAt     DateTime @default(now())
    updatedAt     DateTime @default(now())
}
